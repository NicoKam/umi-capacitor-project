{
  "name": "mobile-cordova",
  "version": "0.0.1",
  "scripts": {
    "cordova-add-ios": "cordova platform add ios",
    "cordova-add-android": "cordova platform add android",
    "android": "umi dev --android",
    "ios": "umi dev --ios",
    "build-android": "umi build --android",
    "build-ios": "umi build --ios",
    "start": "umi dev",
    "build": "umi build",
    "eslint": "eslint --fix --ext .js --ext .jsx --ext .ts --ext .tsx ./src",
    "lint-staged": "lint-staged",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@ant-design/icons": "^4.2.1",
    "@umijs/preset-react": "^1.5.6",
    "@umijs/test": "^3.2.3",
    "antd-mobile": "^2.2.8",
    "classnames": "^2.2.6",
    "cordova-android": "^8.1.0",
    "dva": "^2.4.1",
    "query-string": "5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "routes-watcher": "0.0.8",
    "umi": "^3.2.3",
    "umi-request": "^1.3.3"
  },
  "devDependencies": {
    "@whalecloud/eslint-config": "0.0.22",
    "babel-eslint": "^10.1.0",
    "cordova-plugin-whitelist": "^1.3.4",
    "cross-env": "^5.2.1",
    "lint-staged": "^10.2.9",
    "tar": "^4.4.13",
    "umi-plugin-convention-routes": "0.0.2",
    "yorkie": "^2.0.0"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {}
    },
    "platforms": [
      "android"
    ]
  }
}
